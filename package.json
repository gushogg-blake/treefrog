	"scripts": {
		"start": "electron .",
		"electron": "NODE_ENV=development electron .",
		"build-all": "rollup -c",
		"autobuild-all": "rollup -c -w",
		"build-test": "PLATFORM=test rollup -c",
		"autobuild-test": "PLATFORM=test rollup -c -w",
		"build-electron": "PLATFORM=electron rollup -c",
		"autobuild-electron": "PLATFORM=electron rollup -c -w",
		"build-web": "PLATFORM=web rollup -c",
		"autobuild-web": "PLATFORM=web rollup -c -w",
		"dev": "run-p autobuild-all test electron web",
		"electron-dev": "run-p autobuild-electron test electron",
		"web-dev": "run-p autobuild-web test web",
		"test-dev": "run-p autobuild-test test",
		"web": "sirv --dev --port 5000 src/platforms/web/public",
		"test": "sirv --dev --port 5001 test/public",
		"dist": "run-s build-electron pure-dist-win",
		"pure-dist-win": "electron-builder --win --config electron-builder.yml"
	}
